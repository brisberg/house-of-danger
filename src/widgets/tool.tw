:: Tools [widget]
<<nobr>>
  <<widget "PickupTool">>
    <<pickup '$tools' $args[0]>>
    <<Tool $args[0] "Tooltip Desc">>
  <</widget>>

  <<widget "Tool">>
    <span class='tooltip'>
      $args[0]
      <span class='tooltip-bottom tooltiptext'>
        $args[1]
      </span>
    </span>
  <</widget>>
<</nobr>>
